  <item>
    <appendix>Shortcut keys to open apps.</appendix>
    <identifier>private.shortcut_keys_to_open_apps</identifier>

    <autogen>
      __BlockUntilKeyUp__ KeyCode::RETURN
    </autogen>

    <!-- <autogen> -->
    <!--   __BlockUntilKeyUp__ KeyCode::A, ModifierFlag::OPEN_APPS, -->
    <!-- </autogen> -->


    <autogen>
	    __KeyOverlaidModifier__
      KeyCode::KEYPAD_PLUS, ModifierFlag::NONE,
      KeyCode::VK_MODIFIER_OPEN_APPS,
      KeyCode::L,
    </autogen>




    <autogen>
      __KeyToKey__
      KeyCode::M, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Messages,
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::P, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Preview,
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::BRACKET_LEFT, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Eventviewer,
    </autogen>
    <!-- This opens the currently selected text in slicereader -->
    <autogen>
      __KeyToKey__
      KeyCode::SHIFT_L, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Keynote,
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::KEYPAD_7, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Ibooks,
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::G, ModifierFlag::OPEN_APPS,

      __{ KeyCode::VK_OPEN_URL_APP_Chrome, KeyCode::ESCAPE, KeyCode::VK_WAIT_10MS, KeyCode::T, ModifierFlag::COMMAND_L, KeyCode::VK_WAIT_10MS, KeyCode::A, }__
    </autogen>

    <!-- <autogen> -->
    <!--   __KeyToKey__ -->
    <!--   KeyCode::, ModifierFlag::OPEN_APPS, -->

    <!--   KeyCode::DASHBOARD, -->
    <!-- </autogen> -->

    <autogen>
      __KeyToKey__
      KeyCode::KEYPAD_1, ModifierFlag::OPEN_APPS,

      __ { KeyCode::VK_OPEN_URL_APP_Dash, KeyCode::L, ModifierFlag::COMMAND_L, }__
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::KEYPAD_4, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Chrome,
    </autogen>


    <autogen>
      __KeyToKey__
      KeyCode::KEYPAD_9, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Preview
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::L, ModifierFlag::OPEN_APPS,

      KeyCode::VK_OPEN_URL_APP_Karabiner,
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::KEYPAD_5, ModifierFlag::OPEN_APPS,

      KeyCode::VK_OPEN_URL_APP_Iterm,
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::KEYPAD_6, ModifierFlag::OPEN_APPS,

      KeyCode::VK_OPEN_URL_APP_Emacs,
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::KEYPAD_8, ModifierFlag::OPEN_APPS,

      KeyCode::VK_OPEN_URL_APP_Kindle,
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::J, ModifierFlag::OPEN_APPS,
      KeyCode::VK_OPEN_URL_APP_Slicereader,
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::T, ModifierFlag::OPEN_APPS,

      __{ KeyCode::C, ModifierFlag::COMMAND_L, KeyCode::VK_WAIT_10MS, KeyCode::VK_OPEN_URL_APP_Slicereader, KeyCode::O, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L, KeyCode::VK_WAIT_10MS, KeyCode::V | ModifierFlag::COMMAND_L, KeyCode::VK_WAIT_10MS, KeyCode::COMMA, | ModifierFlag::CONTROL_L, KeyCode::VK_WAIT_10MS, KeyCode::SPACE, KeyCode::SPACE, KeyCode::SPACE, KeyCode::SPACE, KeyCode::SPACE, KeyCode::SPACE, KeyCode::VK_WAIT_50MS, KeyCode::S | ModifierFlag::SHIFT_L, KeyCode::VK_WAIT_50MS, KeyCode::L, KeyCode::VK_WAIT_50MS, KeyCode::I, KeyCode::VK_WAIT_50MS, KeyCode::C, KeyCode::VK_WAIT_50MS, KeyCode::E, KeyCode::VK_WAIT_50MS, KeyCode::RETURN, KeyCode::VK_WAIT_50MS, KeyCode::F | ModifierFlag::COMMAND_R, | ModifierFlag::CONTROL_R }__
    </autogen>

      <autogen>
        __KeyToKey__

        KeyCode::TAB,
        ModifierFlag::OPEN_APPS,

        KeyCode::CURSOR_LEFT, ModifierFlag::OPTION_L,
      </autogen>
      <autogen>
        __KeyToKey__

        KeyCode::C,
        ModifierFlag::OPEN_APPS,

        KeyCode::VK_OPEN_URL_APP_Kindle,
      </autogen>
      <autogen>
        __KeyToKey__

        KeyCode::W,
        ModifierFlag::OPEN_APPS,

        KeyCode::CURSOR_UP, ModifierFlag::OPTION_L, ModifierFlag::COMMAND_L,
      </autogen>

      <autogen>
        __KeyToKey__

        KeyCode::E,
        ModifierFlag::OPEN_APPS,

        KeyCode::CURSOR_RIGHT, ModifierFlag::OPTION_L,
      </autogen>


      <autogen>
        __KeyToKey__

        KeyCode::CONTROL_L, ModifierFlag::CONTROL_L,
        ModifierFlag::OPEN_APPS,

        KeyCode::CURSOR_LEFT,
      </autogen>
      <autogen>
        __KeyToKey__

        KeyCode::A,
        ModifierFlag::OPEN_APPS,

        KeyCode::CURSOR_DOWN,
      </autogen>
      <autogen>
        __KeyToKey__

        KeyCode::S,
        ModifierFlag::OPEN_APPS,

        KeyCode::CURSOR_UP,
      </autogen>

      <autogen>
        __KeyToKey__

        KeyCode::D,
        ModifierFlag::OPEN_APPS,

        KeyCode::CURSOR_RIGHT,
      </autogen>
    </item>
</root>
